<div class="notes">
	{% for note in notes %}
		<div
			class="note">
			{# <span class="material-icons check-circle">check_circle</span> #}
			<div class="title">{{ note.title }}</div>
			<div class="text">{{ note.text }}</div>
			<div class="note-footer">
				{{note.date}}
			</div>
		</div>
	{% endfor %}
</div>
